<%= link_to 'Back', users_skills_path %>

<%= simple_form_for [:users, @skill] do |f| %>

   <%= f.input :name %>
   <%= f.input :proficiency, collection: @skill.proficiencies %>
   <%= f.input :icon, as: :file %>
   <%= f.input :description, as: :rich_text_area %>

   <%= f.button :submit %>

<% end %>